{
  "headers": [
    {
      "source": "/pkg/(.*\\.wasm)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/worker.js",
      "destination": "/worker.js"
    },
    {
      "source": "/pkg/(.*)",
      "destination": "/pkg/$1"
    }
  ]
}

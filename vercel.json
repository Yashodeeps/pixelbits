{
  "version": 2,
  "buildCommand": "bash build.sh",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "routes": [
    {
      "src": "/pkg/(.*)",
      "headers": {
        "content-type": "application/wasm"
      },
      "continue": true
    }
  ]
}
